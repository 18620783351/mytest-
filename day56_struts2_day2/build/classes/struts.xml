<?xml version="1.0" encoding="UTF-8"?>
	<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">
	<struts>
	 	<constant name="struts.devMode" value="true"></constant>	
		
		<!-- 添加用户-->
		<package name="linkman" extends="struts-default" namespace="/abc">
		
			<action name="addLinkManUI" class="com.baidu.action.LinkmanAction" method="addLinkManUI">
			 	<result name="addLinkManUI">/jsp/linkman/add.jsp</result>
			
			</action>				
			<action name="addLinkMan" class="com.baidu.action.LinkmanAction" method="addLinkMan">
				<result name="addLinkMan">/jsp/success.jsp</result>		
			</action>
			
			<action name="addCustomerUI" class="com.baidu.action.CustomerAction" method="addCustomerUI">
			 	<result name="addCustomerUI">/jsp/customer/add.jsp</result>
			
			</action>				
			<action name="addCustomer" class="com.baidu.action.CustomerAction" method="addCustomer">
				<result name="addCustomer">/jsp/success.jsp</result>		
			</action>
			
			<action name="findAllLinkMan" class="com.baidu.action.LinkmanAction" method="findAllLinkMan">
				<result>/jsp/linkman/list.jsp</result>
			</action>
			<action name="findAllCustomer" class="com.baidu.action.CustomerAction" method="findAllCustomer">
				<result>/jsp/customer/list.jsp</result>			
			</action>
		</package>	
	</struts>

	
	